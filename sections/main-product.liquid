<script>
  function addItemToCart(variantID, button) {
    var $button = $(button);
      data = {
        "id": variantID,
        "quantity": 1
      }
   
    jQuery.ajax({
      type: 'POST',
      url: '/cart/add.js',
      data: data,
      dataType: 'json',
      success: function() { 
        $button.parent().find('a.addtocart-product-details-order-sample.rcp-button').hide();
        $button.parent().find('a.addtocart-product-details-purchase-fabric.rcp-button').hide();
        $button.parent().find('a.addtocart-product-details-purchased.rcp-button').show();
        $button.parent().find('a.related-product-details-order-sample.rcp-button').hide();
        $button.parent().find('a.related-product-details-purchase-fabric.rcp-button').hide();
        $button.parent().find('a.related-product-details-purchased.rcp-button').show();
      }
    });
  }
</script>
